  <data name="InsertRecord" xml:space="preserve">
    <value>INSERT INTO dns_record_dmarc
(`id`,
`domain_id`,
`record`,
`end_date`,
`failure_count`,
`result_code`,
`org_domain`,
`is_tld`,
`is_inherited`) VALUES</value>
  </data>
  <data name="InsertRecordOnDuplicateKey" xml:space="preserve">
    <value>ON DUPLICATE KEY UPDATE `end_date` = VALUES(`end_date`),
`failure_count` = VALUES(`failure_count`),
`result_code` = VALUES(`result_code`),
`last_checked` = UTC_TIMESTAMP();</value>
  </data>
  <data name="InsertRecordValueFormatString" xml:space="preserve">
    <value>(@a{0},
@b{0},
@c{0},
@d{0},
@e{0},
@f{0},
@g{0},
@h{0},
@i{0})</value>
  </data>
  <data name="SelectDomainsWithRecords" xml:space="preserve">
    <value>SELECT temp.domain_id,
temp.domain_name,
dr2.id,
dr2.record,
dr2.result_code,
dr2.failure_count,
dr2.org_domain,
dr2.is_tld,
dr2.is_inherited
FROM
(
SELECT d1.id as domain_id,
d1.name as domain_name,
MAX(COALESCE(dr1.last_checked, '1900-01-01')) as lst_checked
FROM domain d1
LEFT JOIN dns_record_dmarc dr1 ON d1.id = dr1.domain_id
WHERE (d1.monitor = b'1' OR d1.publish=b'1') 
AND dr1.end_date IS NULL
AND (dr1.id IS NULL ||
	(UTC_TIMESTAMP() &gt; DATE_ADD(dr1.last_checked, INTERVAL @refreshIntervalSeconds SECOND) &amp;&amp; dr1.failure_count &lt;= 0) ||
	(UTC_TIMESTAMP() &gt; DATE_ADD(dr1.last_checked, INTERVAL @failureRefreshIntervalSeconds SECOND) &amp;&amp; dr1.failure_count &gt; 0))
GROUP BY d1.id
ORDER BY lst_checked, d1.id
LIMIT @limit
) as temp 
LEFT JOIN dns_record_dmarc dr2 ON temp.domain_id = dr2.domain_id
WHERE dr2.end_date IS NULL
ORDER BY lst_checked, temp.domain_id</value>
  </data>
